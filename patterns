#include<stdio.h>
int main ()
{
    int n;
    scanf("%d",&n);
    for( int i=0;i<=n-1;i++)
    {
        for(int j=0;j<=n-1;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}



#include<stdio.h>
int main ()
{
    int n,m;
    scanf("%d%d",&n,&m);
    for( int i=0;i<=n-1;i++)
    {
        for(int j=0;j<=m-1;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}

output:

4 6
******
******
******
******


#include<stdio.h>
int main ()
{
    int n;
    scanf("%d",&n);
    for( int i=0;i<=n-1;i++)
    {
        for(int j=0;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}

output:


5
*
**
***
****
*****

#include<stdio.h>
int main ()
{
    int n;
    scanf("%d",&n);
    for( int i=n;i>=1;i--)
    {
        for(int j=1;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


output:


4
****
***
**
*

#include<stdio.h>
int main ()
{
    int n;
    scanf("%d",&n);
    for( int i=0;i<=n-1;i++)
    {
        for(int j=0;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
   
    for( int i=n-1;i>=1;i--)
    {
        for(int j=1;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}

output:

4
*
**
***
****
***
**
*

#include<stdio.h>
int main ()
{
    int n;
    scanf("%d",&n);
    
    
    for( int i=1;i<=n;i++)
   { 
        for(int j=n-1;j>=i;j--)
        {
            printf(" ");
        }
       
        for(int j=1;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
   }
    
    return 0;
}

output:

4
   *
  **
 ***
****





